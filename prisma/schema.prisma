// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Skin {
    id              Int      @id @default(autoincrement())
    name            String?  @unique
    classid         String?  @unique
    type            String?
    weapon_type     String?
    gun_type        String?
    rarity          String?
    rarity_color    String?
    prices          Json?
    first_sale_date String?
    knife_type      String?
    image           String?
    skinId          String?
    min_float       Float?
    max_float       Float?
    stattrak        Boolean?
}
